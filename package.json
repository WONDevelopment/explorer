{
  "name": "WonExplorer",
  "private": false,
  "version": "0.1.0",
  "description": "A lightweight Won block explorer",
  "repository": "https://github.com/WONDevelopment/explorer",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel appSrv syncSrv",
    "appSrv": "node app.js",
    "syncSrv": "node tools/sync.js ",
    "pretest": "copyfiles app.js app/  ",
    "test": "./node_modules/.bin/mocha --reporter spec  ",
    "dev:mongoUp": "pgrep mongod || mongod --dbpath data --fork --logpath /dev/null",
    "dev:mongoDown": "mongo admin --eval \"db.getSiblingDB('admin').shutdownServer()\""
  },
  "dependencies": {
    "bignumber.js": "^5.0.0",
    "body-parser": "^1.12.2",
    "concurrently": "^3.5.1",
    "npm-run-all": "^4.1.5",
    "ejs": "~2.5.7",
    "express": "^4.16.0",
    "mongoose": "^4.10.8",
    "morgan": "^1.9.0",
    "rlp": "^2.0.0",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.0.4",
    "solc": "^0.4.19",
    "ethereumjs-util": "^6.0.0",
    "number-to-bn": "^1.7.0",
    "request": "^2.83.0",
    "utf8": "^3.0.0",
    "crypto-js": "^3.1.9-1",
    "xhr2": "^0.1.4",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "ganache-cli": "^6.0.3",
    "mocha": "^4.1.0"
  }
}
